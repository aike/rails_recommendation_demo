<h1>新規ブックマーク</h1>

<% form_for(@bookmark) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= label :webpage, "Title" %><br />
    <%= text_field :webpage, :title %>
  </p>
  <p>
    <%= label :webpage, "URL" %><br />
    <%= text_field :webpage, :url %>
  </p>
  <p>
    <%= f.label :comment %><br />
    <%= f.text_field :comment %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', bookmarks_path %>
